                    <!-- Normal Freq -->
                    <Veins name="IronVeins" inherits="PresetHugeVeins">
                        <OreBlock block="minecraft:iron_ore" weight="0.40" />
                        <OreBlock block="beyondreality:sparseiron" weight="0.55" />
                        <OreBlock block="denseores:minecraft_iron_ore_0" weight="0.05" />
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60EBD6E9</WireframeColor>
                        <Setting name="MotherlodeSize" avg="7" range="2" type="normal" />
                        <Setting name="MotherlodeFrequency" avg="0.0020" />
                        <!--How offten it generates-->
                        <Setting name="MotherlodeHeight" avg="45" range="3" />
                        <!--Y Level spawn -->
                        <Setting name="BranchLength" avg="600" range="200" type="normal" />
                        <Setting name="BranchHeightLimit" avg="5" />
                        <Setting name="BranchInclination" avg="0.0" range="0.03" />
                        <Setting name="SegmentAngle" avg="0.30" range="0.30" />
                        <Setting name="SegmentLength" avg="30" range="6" />
                        <Setting name="SegmentForkFrequency" avg="0.01" />
                        <Setting name="SegmentRadius" avg="0.83" range="0.02" />
                    </Veins>
                    <!-- Increased Chances -->
                    <Veins name="IronPrefVeins" inherits="PresetHugeVeins">
                        <OreBlock block="minecraft:iron_ore" weight="0.50" />
                        <OreBlock block="beyondreality:sparseiron" weight="0.40" />
                        <OreBlock block="denseores:minecraft_iron_ore_0" weight="0.10" />
