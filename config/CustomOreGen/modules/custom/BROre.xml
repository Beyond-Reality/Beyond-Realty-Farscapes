                    <!-- diamond -->
                    <StandardGen name="DiamondStandard" block="minecraft:diamond_ore" inherits="PresetStandardGen">
                        <Description> Equivalent to vanilla minecraft iron distribution </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x40221100</WireframeColor>
                        <Setting name="Size" avg="6" range="2" />
                        <Setting name="Frequency" avg="10" />
                        <Setting name="Height" avg="9" range="5" scaleTo="sealevel" />
                    </StandardGen>
                </IfCondition>
                <!-- *************************************************** Custom Dist ********************************************************* -->
                <IfCondition condition=":= FarScapeDist = &quot;FarScapeVeins&quot;">
